<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>CustomerService</servlet-name>
        <servlet-class>com.jatekkolcsonzo.server.services.CustomerServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CustomerService</servlet-name>
        <url-pattern>/JatekKolcsonzo/CustomerService</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>GameService</servlet-name>
        <servlet-class>com.jatekkolcsonzo.server.services.GameServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GameService</servlet-name>
        <url-pattern>/JatekKolcsonzo/GameService</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ReservationService</servlet-name>
        <servlet-class>com.jatekkolcsonzo.server.services.ReservationServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReservationService</servlet-name>
        <url-pattern>/JatekKolcsonzo/ReservationService</url-pattern>
    </servlet-mapping>

</web-app>
